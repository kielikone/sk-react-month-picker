{
  "name": "sk-react-month-picker",
  "version": "0.2.0",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kielikone/sk-react-month-picker.git"
  },
  "description": "SK React month picker",
  "main": "dist/MonthPicker/MonthPicker.js",
  "module": "dist/MonthPicker/MonthPicker.js",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "calendar",
    "monthpicker",
    "month picker",
    "date picker",
    "months"
  ],
  "author": "Kielikone & CAEDMON MULLIN <caedmon.m@gmail.com> (https://caedmonmullin.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kielikone/sk-react-month-picker/issues"
  },
  "homepage": ".",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "dayjs": "^1.11.10",
    "styled-components": "^6.1.8"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts --openssl-legacy-provider test",
    "eject": "react-scripts --openssl-legacy-provider eject",
    "publish:npm": "rm -rf dist && mkdir dist && babel ./src/components -d dist --copy-files",
    "publish:npm_windows": "(if exist dist rmdir /s /q dist) && mkdir dist && babel ./src/components -d dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
